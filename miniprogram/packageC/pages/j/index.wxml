    </text>
	</view>

</view>

<view class="nextStep">
	<image wx:if="{{nowKey == 2 && nowStep == 10 && !MODE}}"
	bindtap="toUpForm"
	class="nextStepImg" mode="heightFix" src="../../../picture/nextStep.png">
	</image>
</view>


